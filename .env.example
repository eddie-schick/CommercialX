# Supabase Configuration (Client-side - Vite)
# Get these from your Supabase project settings: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Configuration (Server-side)
# These should match the client-side values above
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Configuration
# PostgreSQL connection string for Drizzle migrations
DATABASE_URL=postgresql://user:password@host:port/database

# Application Configuration
VITE_APP_ID=your_app_id
JWT_SECRET=your_jwt_secret_key

# OAuth Configuration
OAUTH_SERVER_URL=your_oauth_server_url
OWNER_OPEN_ID=your_owner_open_id

# Forge API Configuration (for storage)
BUILT_IN_FORGE_API_URL=your_forge_api_url
BUILT_IN_FORGE_API_KEY=your_forge_api_key
